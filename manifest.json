{
  "manifest_version": 3,
  "name": "urlsamur.ai",
  "version": "1.O",
  "description": "Crea un link accorciato o un codice QR dalla URL della scheda corrente.",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",

      "48": "icons/icon48.png",js
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }

}
